{
  "compilerOptions": {
    "target": "ES2022",
    "module": "commonjs",
    "lib": ["ES2022"],
    "jsx": "react-jsx",
    "declaration": true,
    "declarationMap": true,
    "sourceMap": true,
    "outDir": "./dist",
    "rootDir": "./",
    "baseUrl": ".",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "resolveJsonModule": true,
    "moduleResolution": "node",
    "allowSyntheticDefaultImports": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "noImplicitReturns": true,
    "noFallthroughCasesInSwitch": true,
    "noUncheckedIndexedAccess": true,
    "exactOptionalPropertyTypes": true,
    "paths": {
      "@ai-accountant/shared-utils": ["packages/shared-utils/src"],
      "@ai-accountant/shared-utils/*": ["packages/shared-utils/src/*"],
      "@ai-accountant/shared-types": ["packages/shared-types/src"],
      "@ai-accountant/shared-types/*": ["packages/shared-types/src/*"],
      "@ai-accountant/secure-store": ["packages/secure-store/src"],
      "@ai-accountant/secure-store/*": ["packages/secure-store/src/*"],
      "@ai-accountant/hmrc": ["packages/hmrc/src"],
      "@ai-accountant/hmrc/*": ["packages/hmrc/src/*"],
      "@ai-accountant/database": ["services/database/src"],
      "@ai-accountant/database/*": ["services/database/src/*"],
      "@ai-accountant/web": ["apps/web/src"],
      "@ai-accountant/web/*": ["apps/web/src/*"],
      "@ai-accountant/mobile": ["apps/mobile/src"],
      "@ai-accountant/mobile/*": ["apps/mobile/src/*"],
      "@ai-accountant/accountant-service": ["services/accountant/src"],
      "@ai-accountant/accountant-service/*": ["services/accountant/src/*"],
      "@ai-accountant/analytics-service": ["services/analytics/src"],
      "@ai-accountant/analytics-service/*": ["services/analytics/src/*"],
      "@ai-accountant/api-gateway": ["services/api-gateway/src"],
      "@ai-accountant/api-gateway/*": ["services/api-gateway/src/*"],
      "@ai-accountant/assistant-service": ["services/assistant/src"],
      "@ai-accountant/assistant-service/*": ["services/assistant/src/*"],
      "@ai-accountant/auth-service": ["services/auth/src"],
      "@ai-accountant/auth-service/*": ["services/auth/src/*"],
      "@ai-accountant/automation-service": ["services/automation/src"],
      "@ai-accountant/automation-service/*": ["services/automation/src/*"],
      "@ai-accountant/backup-service": ["services/backup/src"],
      "@ai-accountant/backup-service/*": ["services/backup/src/*"],
      "@ai-accountant/bank-feed-service": ["services/bank-feed/src"],
      "@ai-accountant/bank-feed-service/*": ["services/bank-feed/src/*"],
      "@ai-accountant/billing-service": ["services/billing/src"],
      "@ai-accountant/billing-service/*": ["services/billing/src/*"],
      "@ai-accountant/cache-service": ["services/cache/src"],
      "@ai-accountant/cache-service/*": ["services/cache/src/*"],
      "@ai-accountant/classification-service": ["services/classification/src"],
      "@ai-accountant/classification-service/*": ["services/classification/src/*"],
      "@ai-accountant/compliance-service": ["services/compliance/src"],
      "@ai-accountant/compliance-service/*": ["services/compliance/src/*"],
      "@ai-accountant/document-ingest-service": ["services/document-ingest/src"],
      "@ai-accountant/document-ingest-service/*": ["services/document-ingest/src/*"],
      "@ai-accountant/error-handling-service": ["services/error-handling/src"],
      "@ai-accountant/error-handling-service/*": ["services/error-handling/src/*"],
      "@ai-accountant/filing-service": ["services/filing/src"],
      "@ai-accountant/filing-service/*": ["services/filing/src/*"],
      "@ai-accountant/integrations-service": ["services/integrations/src"],
      "@ai-accountant/integrations-service/*": ["services/integrations/src/*"],
      "@ai-accountant/ledger-service": ["services/ledger/src"],
      "@ai-accountant/ledger-service/*": ["services/ledger/src/*"],
      "@ai-accountant/monitoring-service": ["services/monitoring/src"],
      "@ai-accountant/monitoring-service/*": ["services/monitoring/src/*"],
      "@ai-accountant/notification-service": ["services/notification/src"],
      "@ai-accountant/notification-service/*": ["services/notification/src/*"],
      "@ai-accountant/ocr-service": ["services/ocr/src"],
      "@ai-accountant/ocr-service/*": ["services/ocr/src/*"],
      "@ai-accountant/onboarding-service": ["services/onboarding/src"],
      "@ai-accountant/onboarding-service/*": ["services/onboarding/src/*"],
      "@ai-accountant/reconciliation-service": ["services/reconciliation/src"],
      "@ai-accountant/reconciliation-service/*": ["services/reconciliation/src/*"],
      "@ai-accountant/reporting-service": ["services/reporting/src"],
      "@ai-accountant/reporting-service/*": ["services/reporting/src/*"],
      "@ai-accountant/resilience-service": ["services/resilience/src"],
      "@ai-accountant/resilience-service/*": ["services/resilience/src/*"],
      "@ai-accountant/rules-engine-service": ["services/rules-engine/src"],
      "@ai-accountant/rules-engine-service/*": ["services/rules-engine/src/*"],
      "@ai-accountant/security-service": ["services/security/src"],
      "@ai-accountant/security-service/*": ["services/security/src/*"],
      "@ai-accountant/support-service": ["services/support/src"],
      "@ai-accountant/support-service/*": ["services/support/src/*"],
      "@ai-accountant/validation-service": ["services/validation/src"],
      "@ai-accountant/validation-service/*": ["services/validation/src/*"],
      "@ai-accountant/performance": ["services/performance/src"],
      "@ai-accountant/performance/*": ["services/performance/src/*"],
        "@ai-accountant/workflow-service": ["services/workflow/src"],
        "@ai-accountant/workflow-service/*": ["services/workflow/src/*"],
        "@ai-accountant/observability": ["packages/observability/src"],
        "@ai-accountant/observability/*": ["packages/observability/src/*"]
    }
  },
  "exclude": ["node_modules", "dist", "build", ".next"]
}
